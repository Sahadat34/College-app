<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.collageapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CollageApp">
        <activity
            android:name="com.example.collegeapp.Developer"
            android:label="Developer"
            android:parentActivityName="com.example.collegeapp.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.example.collegeapp.gallery.AllImage"
            android:exported="false"
            android:label="Gallery"
            android:parentActivityName="com.example.collegeapp.gallery.GalleryActivity" />
        <activity
            android:name="com.example.collegeapp.gallery.GalleryActivity"
            android:exported="false"
            android:label="Gallery"
            android:parentActivityName="com.example.collegeapp.MainActivity" />
        <activity
            android:name="com.example.collegeapp.NoticeDetailsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.collegeapp.DepartmentDetailsActivity"
            android:exported="false"
            android:parentActivityName="com.example.collegeapp.MainActivity" />
        <activity
            android:name="com.example.collegeapp.ebook.PdfViewActivity"
            android:exported="false"
            android:label="PDf Viewer"
            android:parentActivityName="com.example.collegeapp.MainActivity" />
        <activity
            android:name="com.example.collegeapp.ebook.PdfActivity"
            android:exported="false"
            android:label="PDF"
            android:parentActivityName="com.example.collegeapp.MainActivity" />
        <activity
            android:name="com.example.collegeapp.WebsiteActivity"
            android:exported="false"
            android:label="Website"
            android:parentActivityName="com.example.collegeapp.MainActivity" />
        <activity
            android:name="com.example.collegeapp.MainActivity"
            android:exported="true"
            android:label="Bitm App">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>